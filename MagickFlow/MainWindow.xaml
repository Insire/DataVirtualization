<Window x:Class="MagickFlow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:browser="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:MagickFlow"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="525"
        Height="350"
        mc:Ignorable="d">
    <DockPanel LastChildFill="True">
        <browser:ChromiumWebBrowser x:Name="Browser"
                                    IsBrowserInitializedChanged="Browser_IsBrowserInitializedChanged" />
    </DockPanel>
</Window>
